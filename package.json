    "crypto-js": "^4.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@mymind/duc-banh-mi": "0.0.1",
    "@types/crypto-js": "^4.2.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "dotenv": "^16.0.1",
    "@biomejs/biome": "2.1.3",
    "husky": "^7.0.4",
    "sass": "^1.54.4",
    "typescript": "5.1.6",
    "vite": "5.4.11",
