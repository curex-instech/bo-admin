{
  "private": true,
  "name": "bo-web-admin",
  "version": "0.0.1",
  "main": "index.html",
  "scripts": {
    "start": "vite",
    "start:no-typecheck": "TS_CHECKER=off vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:analyze": "echo TODO",
    "lint": "biome lint ./src",
    "format": "biome format ./src --write"
  },
  "banhMi": {
    "publicPath": "/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "biome check --apply --no-errors-on-unmatched --files-ignore-unknown=true $(git diff --cached --name-only --diff-filter=ACMR \"*.ts\" \"*.tsx\" \"*.js\" \"*.json\")"
    }
  },
  "license": "ISC",
  "dependencies": {
    "@mymind/banh-mi": "0.0.1",
